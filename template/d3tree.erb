<style type="text/css">

.node {
	cursor: pointer;
}

.overlay{
	background-color:#EEE;
}

.node circle {
	fill: #fff;
	stroke: steelblue;
	stroke-width: 1.5px;
}

.node text {
	font-size:9px; 
	font-family:sans-serif;
}

.link {
	fill: none;
	stroke: red; //#ccc;
	stroke-width: 1.5px;
}

.templink {
	fill: none;
	stroke: red;
	stroke-width: 3px;
}

.ghostCircle.show{
	display:block;
}

.ghostCircle, .activeDrag .ghostCircle{
	display: none;
}

</style>
<script src="https://d3js.org/d3.v3.min.js"></script>
<script type="text/javascript" src="<%= @param['baseurl'] %>/files/dndtree.js"></script>
<div id="tree-container"></div>
